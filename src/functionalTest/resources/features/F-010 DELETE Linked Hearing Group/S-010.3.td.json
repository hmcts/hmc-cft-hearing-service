{
  "_guid_": "S-010.3",
  "_extends_" : "F-010_TestBase",

  "specs": [
    "an active profile in CCD",
    "has 404 status code",
    "has incorrect request invocation"
  ],

  "request": {
    "pathVariables": {
      "id": "987"
    },
    "body": {
      "groupDetails": {
        "groupName": "groupName",
        "groupReason": "reason",
        "groupLinkType": "Same Slot",
        "groupComments": "groupComments"
      },
      "hearingsInGroup": [
        {
          "hearingId": "${[scenarioContext][childContexts][CreateHearingRequest][testData][actualResponse][body][hearingRequestID]}",
          "hearingOrder": "1"
        },
        {
          "hearingId": "${[scenarioContext][childContexts][CreateAnotherLinkedHearingRequest][testData][actualResponse][body][hearingRequestID]}",
          "hearingOrder": "2"
        }
      ]
    }
  },

  "expectedResponse": {
    "responseCode": 404,
    "responseMessage": "Not Found",
    "body" : {
      "status" : "NOT_FOUND",
      "errors" : [ "Invalid linked group id" ]
    }
  }
}
